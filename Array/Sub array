#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    ll n, k; cin >> n >> k;
    vector<ll> arr(n);
    for(ll i = 0; i < n; i++) cin >> arr[i];

    for(ll i = 0; i <= n -1; i++){
        for(ll j = i; j <= n-1; j++){
            for(ll k = i; k < j+1; k++){
                cout << arr[k] << " ";
            }
            cout << endl;
        }
    }




}
